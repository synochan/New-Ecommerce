{
    "builds": [
        {
            "src": "backend/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/media/(.*)",
            "dest": "/media/$1"
        },
        {
            "src": "/(.*)",
            "dest": "backend/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "backend.settings"
    }
} 